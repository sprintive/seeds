{
  "name": "sprintive/seeds",
  "description": "Kickoff distro for SME's, Curated by Sprintive",
  "type": "drupal-profile",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6",
    "composer/installers": "1.4.0",
    "cweagans/composer-patches": "1.6.2",
    "drupal-composer/drupal-scaffold": "2.3.0",
    "drupal/drupal-library-installer-plugin": "0.3",
    "drupal/core": "8.3.7",
    "drupal/ctools": "~3.0",
    "drupal/token": "~1.0",
    "drupal/ds": "~2.6",
    "drupal/page_manager": "~1.0",
    "drupal/panels": "~3.0",
    "drupal/layout_plugin": "~1.0",
    "drupal/entity": "~1.0",
    "drupal/entity_browser": "~1.0",
    "drupal/entity_embed": "~1.0",
    "drupal/embed": "~1.0",
    "drupal/inline_entity_form": "~1.0",
    "drupal/media_entity": "~1.6",
    "drupal/media_entity_browser": "~1.0",
    "drupal/media_entity_image": "~1.2",
    "drupal/video_embed_field": "~1.3",
    "drupal/metatag": "~1.0",
    "drupal/pathauto": "~1.0",
    "drupal/simple_sitemap": "~2.7",
    "drupal/link_attributes": "~1.0"
  },
  "require-dev": {
    "drupal-composer/drupal-scaffold": "~2.2",
    "drupal/coder": "~8.2",
    "drupal/config_devel": "~1.0",
    "drupal/console": "~0.11",
    "drush/drush": "~8.1"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "extra": {
    "enable-patching": true,
    "installer-paths": {
      "public_html/core": [
        "drupal/core"
      ],
      "public_html/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "public_html/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "public_html/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    }
  },
  "config": {
    "bin-dir": "bin/"
  },
  "scripts": {
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ],
    "post-update-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ]
  },
  "authors": [
    {
      "name": "Mohammad Abdul-Qader",
      "homepage": "https://www.drupal.org/u/mabdulqader",
      "role": "Maintainer"
    },
    {
      "name": "Mohammad AlQanneh",
      "homepage": "https://www.drupal.org/u/mqanneh",
      "role": "Maintainer"
    }
  ]
}
