{
  "name": "sprintive/seeds",
  "description": "Kickoff distro for SME's, Curated by Sprintive",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6",
    "composer/composer": "^1.3",
    "composer/installers": "1.4.0",
    "cweagans/composer-patches": "1.6.2",
    "drupal-composer/drupal-scaffold": "2.3.0",
    "drupal/drupal-library-installer-plugin": "0.3",
    "drupal/core": "8.5.1",
    "drupal/ctools": "3.0",
    "drupal/token": "1.1",
    "drupal/diff": "1.0-rc1",
    "drupal/ds": "3.1",
    "drupal/panels": "4.2",
    "drupal/bootstrap_layouts": "5.1",
    "drupal/entity": "1.0-beta3",
    "drupal/entity_browser": "1.4",
    "drupal/entity_embed": "1.0-beta2",
    "drupal/embed": "1.0",
    "drupal/inline_entity_form": "1.0-beta1",
    "drupal/media_entity_actions": "1.0-alpha2",
    "drupal/media_entity_generic": "1.0-alpha1",
    "drupal/media_entity_image_exif": "1.x-dev",
    "drupal/video_embed_field": "2.0-alpha2",
    "drupal/crop": "1.5",
    "drupal/focal_point": "1.0-beta6",
    "drupal/smart_trim": "1.1",
    "drupal/allowed_formats": "1.1",
    "drupal/slick": "1.0",
    "drupal/blazy": "1.0-rc2",
    "drupal/slick_views": "1.0-rc2",
    "drupal/slick_media": "1.0",
    "drupal/entity_reference_revisions": "1.4",
    "drupal/paragraphs": "1.2",
    "drupal/editor_advanced_link": "1.4",
    "drupal/contribute": "1.0-beta7",
    "drupal/webform": "5.0-rc9",
    "drupal/rabbit_hole": "1.0-beta4",
    "drupal/field_group": "1.0",
    "drupal/photoswipe": "1.0-beta4",
    "drupal/smtp": "1.0-beta3",
    "drupal/fast_404": "1.0-alpha2",
    "drupal/metatag": "1.5",
    "drupal/pathauto": "1.1",
    "drupal/simple_sitemap": "2.12",
    "drupal/link_attributes": "1.2",
    "drupal/redirect": "1.1",
    "drupal/google_analytics": "2.2",
    "drupal/honeypot": "1.27",
    "drupal/adminimal_admin_toolbar": "1.5",
    "drupal/admin_toolbar": "1.23",
    "drupal/toolbar_anti_flicker": "2.6",
    "drupal/bootstrap": "3.10",
    "drupal/thunder_admin": "2.0-beta13",
    "drupal/menu_admin_per_menu": "1.0",
    "drupal/taxonomy_access_fix": "2.6",
    "drupal/responsive_preview": "1.0-alpha7",
    "drupal/views_bulk_operations": "2.2",
    "drupal/views_bulk_edit": "2.0-rc2"
  },
  "require-dev": {
    "drush/drush": "~8.1",
    "drupal/coder": "8.2.12",
    "symfony/css-selector": "~2.8"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "conflict": {
    "drupal/drupal": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-5.x": "8.5.x-dev"
    },
    "installer-paths": {
      "public_html/core": [
        "type:drupal-core"
      ],
      "public_html/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "public_html/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "public_html/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "public_html/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": false,
    "patches": {
      "drupal/ds": {
        "Use absolute or external URL for link wrappers":
                "https://www.drupal.org/files/issues/use_absolute_or-2883928-7.patch"
      },
      "drupal/photoswipe": {
        "Aspect ratio incorrect for landscape photo":
                "https://www.drupal.org/files/issues/aspect_ratio_incorrect-2695191-7.patch"
      },
      "drupal/smtp": {
        "Wrong schema for smtp settings": "https://www.drupal.org/files/issues/2865696-fix-schema-2.patch"
      },
      "drupal/paragraphs": {
        "[META] Support translatable paragraph entity reference revision field.": "https://www.drupal.org/files/issues/meta_support-2461695-224.patch"
      }
    }
  },
  "config": {
    "bin-dir": "bin/"
  },
  "scripts": {
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ],
    "post-update-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ]
  },
  "authors": [
    {
      "name": "Sprintive",
      "homepage": "http://sprintive.com",
      "role": "Supporting Organization"
    },
    {
      "name": "Mohammad Abdul-Qader",
      "homepage": "https://www.drupal.org/u/mabdulqader",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/seeds",
    "source": "http://cgit.drupalcode.org/seeds",
    "email": "info@sprintive.com"
  }
}
