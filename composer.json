{
	"name": "sprintive/seeds",
    "type": "drupal-profile",
    "description": "Kickoff distro for SME's, Curated by Sprintive",
    "license": "GPL-2.0+",
    "require": {
        "composer/installers": "~1.2",
        "cweagans/composer-patches": "~1.5",
        "drupal/core": "~8.2.3",
        "drupal/ctools": "~3.0",
        "drupal/token": "~1.0",
        "drupal/ds": "~2.6",
        "drupal/page_manager": "~1.0",
        "drupal/panels": "~3.0",
        "drupal/layout_plugin": "~1.0",
        "drupal/entity": "~1.0",
        "drupal/entity_browser": "~1.0",
        "drupal/entity_embed": "~1.0",
        "drupal/embed": "~1.0",
        "drupal/inline_entity_form": "~1.0",
        "drupal/media_entity": "~1.6",
        "drupal/media_entity_browser": "~1.0",
        "drupal/media_entity_image": "~1.2",
        "drupal/video_embed_field": "~1.3",
        "drupal/metatag": "~1.0",
        "drupal/pathauto": "~1.0",
        "drupal/simple_sitemap": "~2.7",
        "drupal/link_attributes": "~1.0"
    },
    "require-dev": {
        "drupal-composer/drupal-scaffold": "~2.2",
        "drupal/coder": "~8.2",
        "drupal/config_devel": "~1.0",
        "drupal/console": "~0.11",
        "drush/drush": "~8.1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "extra": {
        "installer-paths": {
            "app/core": [
                "drupal/core"
            ],
            "app/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "app/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "app/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/page_manager": {
                "Fix contextual links on page manager pages": "https://www.drupal.org/files/issues/page_manager-block_content_contextual-2601004-14.patch"
            },
            "drupal/media_entity_browser": {
                "Add view as a config dependency": "https://www.drupal.org/files/issues/meb_add_config-2850574-2.patch"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin/"
    },
    "scripts": {
        "post-install-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ],
        "post-update-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ]
    }
}

