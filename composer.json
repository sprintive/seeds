{
  "name": "sprintive/seeds",
  "description": "Kickoff distro for SME's, Curated by Sprintive",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "composer/composer": "^1.3",
    "composer/installers": "1.4.0",
    "cweagans/composer-patches": "^1.6.4",
    "drupal-composer/drupal-scaffold": "2.3.0",
    "drupal/drupal-library-installer-plugin": "0.3",
    "drupal/core": "8.7.4",
    "drupal/ctools": "^3.2",
    "drupal/token": "^1.5",
    "drupal/diff": "^1.0",
    "drupal/ds": "^3.3",
    "drupal/page_manager": "^4.0",
    "drupal/panels": "^4.4",
    "drupal/bootstrap_layouts": "^5.1",
    "drupal/entity": "^1.0",
    "drupal/entity_browser": "^2.1",
    "drupal/entity_embed": "^1.0",
    "drupal/embed": "^1.0",
    "drupal/inline_entity_form": "^1.0",
    "drupal/media_entity_generic": "^1.0",
    "drupal/video_embed_field": "^2.1",
    "drupal/crop": "^1.5",
    "drupal/focal_point": "^1.0",
    "drupal/smart_trim": "^1.1",
    "drupal/allowed_formats": "^1.1",
    "drupal/slick": "^2.0",
    "drupal/blazy": "^2.0",
    "drupal/slick_views": "^2.0",
		"drupal/slick_media": "^2.0",
		"drupal/slick_paragraphs": "^2.0",
    "drupal/entity_reference_revisions": "^1.6",
    "drupal/paragraphs": "^1.8",
    "drupal/editor_advanced_link": "^1.4",
    "drupal/webform": "^5.2",
    "drupal/rabbit_hole": "^1.0",
    "drupal/field_group": "^3.0",
    "drupal/photoswipe": "^2.9",
    "drupal/smtp": "^1.0",
    "drupal/field_formatter": "^1.2",
    "drupal/layout_builder_restrictions": "^2.0",
    "drupal/linkit": "^5.0",
    "drupal/fast_404": "^1.0",
    "drupal/metatag": "^1.8",
    "drupal/pathauto": "^1.4",
    "drupal/simple_sitemap": "^3.2",
    "drupal/link_attributes": "^1.8",
    "drupal/redirect": "^1.3",
    "drupal/google_analytics": "^2.4",
		"drupal/yoast_seo": "^1.5",
		"drupal/length_indicator": "^1.0",
    "drupal/honeypot": "^1.29",
    "drupal/admin_toolbar": "^1.27",
		"drupal/bootstrap": "^3.19",
		"drupal/bootstrap_barrio": "^4.22",
    "drupal/menu_admin_per_menu": "^1.0",
    "drupal/taxonomy_access_fix": "^2.6",
    "drupal/block_content_permissions": "^1.6",
    "drupal/responsive_preview": "^1.0",
    "drupal/views_bulk_operations": "^2.5",
    "drupal/views_bulk_edit": "^2.3",
    "drupal/masquerade": "^2.0",
		"drupal/cloudflare": "^1.0",
		"drupal/root": "^1.2",
		"drupal/seeds_toolbar": "^1.4",
		"drupal/imageapi_optimize": "^2.0",
		"drupal/imageapi_optimize_resmushit": "^1.0",
		"drupal/ace_editor": "^1.1",
		"drupal/ckeditor_bidi": "^2.1"
  },
  "require-dev": {
    "drush/drush": "~8.1",
    "drupal/coder": "8.2.12",
    "symfony/css-selector": "~2.8"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "conflict": {
    "drupal/drupal": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-7.x": "8.7.x-dev"
    },
    "installer-paths": {
      "public_html/core": [
        "type:drupal-core"
      ],
      "public_html/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "public_html/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "public_html/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "public_html/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": false,
    "patches": {
			"drupal/core": {
				"Issue #2771361: Fix multiple ajax calls problem, which causes the browser to be in an infinite loop thus causing a crash.":
				"https://www.drupal.org/files/issues/2018-03-29/2771361-27.patch",
				"Issue #2985168: Allow media items to be edited in a modal when using the field widget":
				"https://www.drupal.org/files/issues/2019-05-06/2985168-11.patch"
			},
      "drupal/bootstrap_layouts": {
        "Issue #3018633: Incompatible with Layout builder drag and drop":
        "https://www.drupal.org/files/issues/2018-12-06/3018633-draggable.patch",
        "Issue #3010952: Add a container option":
				"https://www.drupal.org/files/issues/2018-11-21/3010952-bootstrap-container-3.patch",
				"Issues #2863131: Add Bootstrap 4 support":
				"https://www.drupal.org/files/issues/2863131-bs4-layouts.patch"
      },
      "drupal/ds": {
        "Issue #2883928: Use absolute or external URL for link wrappers":
        "https://www.drupal.org/files/issues/2018-05-03/use_absolute_or-2883928-9.patch"
      },
      "drupal/block_content_permissions": {
        "Issue #2920739: Allow accessing the Custom block library page without Administer blocks permission":
        "https://www.drupal.org/files/issues/2018-03-17/block_content_permissions-2920739-16.patch"
			},
			"drupal/length_indicator": {
				"Issue #3048286: Add cursor position direction support for RTL":
				"https://www.drupal.org/files/issues/2019-04-15/3048286-3.patch"
			},
			"drupal/responsive_preview": {
				"Issue #2944727: Drupal.debounce is not a function / Drupal.displace is not a function":
				"https://www.drupal.org/files/issues/2018-11-16/2944727-3.patch"
			}
    }
  },
  "config": {
    "bin-dir": "bin/"
  },
  "scripts": {
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ],
    "post-update-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ]
  },
  "authors": [
    {
      "name": "Sprintive",
      "homepage": "http://sprintive.com",
      "role": "Supporting Organization"
    },
    {
      "name": "Mohammad Abdul-Qader",
      "homepage": "https://www.drupal.org/u/mabdulqader",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/seeds",
    "source": "http://cgit.drupalcode.org/seeds",
    "email": "info@sprintive.com"
  }
}
