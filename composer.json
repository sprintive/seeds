{
  "name": "sprintive/seeds",
  "description": "Kickoff distro for SME's, Curated by Sprintive",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "composer/composer": "^1.3",
    "cweagans/composer-patches": "^1.6.4",
    "drupal/core": "^8.9.16",
    "drupal/entity": "1.2",
    "drupal/entity_reference_revisions": "1.9",
    "drupal/ctools": "3.7",
    "drupal/token": "1.9",
    "drupal/webform": "5.26",
    "drupal/ds": "3.12",
    "drupal/field_group": "3.1",
    "drupal/linkit": "5.0-beta12",
    "drupal/seeds_layouts": "1.5",
    "drupal/crop": "2.1",
    "drupal/focal_point": "1.5",
    "drupal/smart_trim": "1.3",
    "drupal/allowed_formats": "1.3",
    "drupal/paragraphs": "1.12",
    "drupal/slick": "2.3",
    "drupal/blazy": "2.4",
    "drupal/slick_views": "2.4",
    "drupal/slick_paragraphs": "2.1",
    "drupal/photoswipe": "2.9",
    "drupal/button_formatter": "1.2",
    "drupal/layout_builder_restrictions": "2.8",
    "drupal/layout_builder_modal": "1.1",
    "drupal/shariff": "1.7",
    "drupal/admin_toolbar": "2.4",
    "drupal/seeds_toolbar": "1.15",
    "drupal/media_embeddable": "1.0.1",
    "drupal/rabbit_hole": "1.0-beta10",
    "drupal/menu_admin_per_menu": "1.3",
    "drupal/taxonomy_access_fix": "3.x-dev#a0f825c0934dbbbc08abcf0845235a97ec7c4aba",
    "drupal/block_content_permissions": "1.10",
    "drupal/masquerade": "2.0-beta4",
    "drupal/responsive_preview": "1.0",
    "drupal/views_bulk_operations": "3.13",
    "drupal/views_bulk_edit": "2.6",
    "drupal/diff": "1.0",
    "drupal/embed": "1.4",
    "drupal/entity_embed": "1.1",
    "drupal/editor_advanced_link": "1.8",
    "drupal/ace_editor": "1.3",
    "drupal/ckeditor_bidi": "2.1",
    "drupal/ckeditor_iframe": "2.1",
    "drupal/fakeobjects": "1.1",
    "drupal/fast_404": "2.x-dev#5382a99335ee466f8261dc1774a3d56f1699da81",
    "drupal/metatag": "1.16",
    "drupal/pathauto": "1.8",
    "drupal/simple_sitemap": "3.10",
    "drupal/link_attributes": "1.11",
    "drupal/redirect": "1.6",
    "drupal/google_analytics": "2.5",
    "drupal/yoast_seo": "1.7",
    "drupal/length_indicator": "1.1",
    "drupal/imageapi_optimize": "2.0-beta1",
    "drupal/imageapi_optimize_resmushit": "1.0-beta1",
    "drupal/cloudflare": "1.0-beta2",
    "drupal/ultimate_cron": "2.0-alpha5",
    "drupal/smtp": "1.0",
    "drupal/captcha": "1.2",
    "drupal/recaptcha": "2.5",
    "drupal/password_policy": "3.0-beta1",
    "drupal/username_enumeration_prevention": "1.2",
    "drupal/events_log_track": "1.1",
    "drupal/mass_pwreset": "1.0-alpha5",
    "drupal/restrict_by_ip": "4.x-dev#e491eb8f628687f304d77f38496b62f2f0062c43",
    "drupal/login_security": "1.5",
    "drupal/bootstrap_barrio": "4.33",
    "drupal/seeds_coat": "1.8",
    "drupal/root": "1.7"
  },
  "require-dev": {
    "drush/drush": "*",
    "drupal/coder": "8.2.12",
    "symfony/css-selector": "~2.8"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "conflict": {
    "drupal/drupal": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-7.x": "8.7.x-dev"
    },
    "installer-paths": {
      "public_html/core": [
        "type:drupal-core"
      ],
      "public_html/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "public_html/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "public_html/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "public_html/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": false,
    "patches": {
      "drupal/core": {
        "Issue #2771361: Fix multiple ajax calls problem, which causes the browser to be in an infinite loop thus causing a crash.": "https://www.drupal.org/files/issues/2018-03-29/2771361-27.patch",
        "Issue #2985168: Allow media items to be edited in a modal when using the field widget": "https://www.drupal.org/files/issues/2021-05-17/drupal-n2985168-69-89x.patch"
      },
      "drupal/focal_point": {
        "Issue #3094478: Integrate focal point with media_library": "https://www.drupal.org/files/issues/2020-01-09/3094478-10.patch"
      },
      "drupal/block_content_permissions": {
        "Issue #2920739: Allow accessing the Custom block library page without Administer blocks permission": "https://www.drupal.org/files/issues/2018-03-17/block_content_permissions-2920739-16.patch"
      },
      "drupal/shariff": {
        "Issue #3060551: data-url should be the url of the node.": "https://www.drupal.org/files/issues/2019-06-10/shariff-node-data-url-3060551-3.patch"
      },
      "drupal/paragraphs": {
        "Issue #2901390: Integrity constraint violation: 1048 Column 'langcode' cannot be null": "https://www.drupal.org/files/issues/2019-08-10/paragraphs-set_langcode_widgets-290139_updated.patch"
      },
      "drupal/recaptcha": {
        "Issue #2493183: Ajax support / Use behaviors for 2.x": "https://www.drupal.org/files/issues/2020-01-30/recaptcha-ajax-2493183-197_0.patch"
      },
      "drupal/yoast_seo": {
        "Issue #3204023: Style issues on mobile": "https://www.drupal.org/files/issues/2021-03-17/style_issues_on_mobile-3204023-3.patch",
        "Issue #3125252: I can't translate SEO Content analysis": "https://www.drupal.org/files/issues/2021-03-17/i_cant_translate_seo_content_analysis-3125252-4.patch"
      },
      "drupal/photoswipe": {
        "Issue #2980534: Support for Drupal 8 responsive images": "https://www.drupal.org/files/issues/2021-02-09/photoswipe-2980534-24.patch"
      },
      "drupal/fast_404": {
        "Issue #2370323: Translatable html error page": "https://www.drupal.org/files/issues/2021-05-18/2370323-12.patch"
      }
    }
  },
  "config": {
    "bin-dir": "bin/"
  },
  "scripts": {
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ],
    "post-update-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ]
  },
  "authors": [
    {
      "name": "Sprintive",
      "homepage": "http://sprintive.com",
      "role": "Supporting Organization"
    },
    {
      "name": "Mohammad Abdul-Qader",
      "homepage": "https://www.drupal.org/u/mabdulqader",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/seeds",
    "source": "http://cgit.drupalcode.org/seeds",
    "email": "info@sprintive.com"
  }
}
