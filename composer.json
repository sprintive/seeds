{
  "name": "sprintive/seeds",
  "description": "Kickoff distro for SME's, Curated by Sprintive",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "composer/composer": "^1.3",
    "composer/installers": "1.4.0",
    "cweagans/composer-patches": "1.6.2",
    "drupal-composer/drupal-scaffold": "2.3.0",
    "drupal/drupal-library-installer-plugin": "0.3",
    "drupal/core": "8.6.2",
    "drupal/ctools": "3.0",
    "drupal/token": "1.5",
    "drupal/diff": "1.0-rc2",
    "drupal/ds": "3.x-dev#fe1da24ab36369ea2227b6e2c026fe8fb9ca1ef0",
    "drupal/page_manager": "4.0-beta3",
    "drupal/panels": "4.3",
    "drupal/bootstrap_layouts": "5.1",
    "drupal/entity": "1.0-rc1",
    "drupal/entity_browser": "2.0",
    "drupal/entity_embed": "1.0-beta2",
    "drupal/embed": "1.0",
    "drupal/inline_entity_form": "1.0-rc1",
    "drupal/media_entity_actions": "1.0-alpha2",
    "drupal/media_entity_generic": "1.0-alpha1",
    "drupal/media_entity_image_exif": "1.x-dev",
    "drupal/video_embed_field": "2.0",
    "drupal/crop": "1.5",
    "drupal/focal_point": "1.0-beta6",
    "drupal/smart_trim": "1.1",
    "drupal/allowed_formats": "1.1",
    "drupal/slick": "1.0",
    "drupal/blazy": "1.0-rc2",
    "drupal/slick_views": "1.0-rc2",
    "drupal/slick_media": "2.0-alpha2",
    "drupal/entity_reference_revisions": "1.6",
    "drupal/paragraphs": "1.3",
    "drupal/editor_advanced_link": "1.4",
    "drupal/contribute": "1.0-beta7",
    "drupal/webform": "5.0-rc24",
    "drupal/rabbit_hole": "1.0-beta4",
    "drupal/field_group": "1.0",
    "drupal/photoswipe": "2.x-dev#34b844a42f8c9dd6283a6e5c682e431f9bd2654c",
    "drupal/smtp": "1.0-beta4",
    "drupal/field_formatter": "1.2",
    "drupal/layout_builder_restrictions": "1.2",
    "drupal/fast_404": "1.0-alpha3",
    "drupal/metatag": "1.7",
    "drupal/pathauto": "1.3",
    "drupal/simple_sitemap": "2.12",
    "drupal/link_attributes": "1.5",
    "drupal/redirect": "1.3",
    "drupal/google_analytics": "2.3",
    "drupal/yoast_seo": "2.0-alpha3",
    "drupal/honeypot": "1.29",
    "drupal/security_review": "1.x-dev#9b8a34a21cac85913845df4eebb9a05c69de82d8",
    "drupal/adminimal_admin_toolbar": "1.8",
    "drupal/admin_toolbar": "1.24",
    "drupal/toolbar_anti_flicker": "2.7",
    "drupal/bootstrap": "3.13",
    "drupal/thunder_admin": "2.1",
    "drupal/menu_admin_per_menu": "1.0",
    "drupal/taxonomy_access_fix": "2.6",
    "drupal/responsive_preview": "1.0-alpha7",
    "drupal/views_bulk_operations": "2.4",
    "drupal/views_bulk_edit": "2.2",
    "drupal/masquerade": "2.0-beta2",
    "drupal/cloudflare": "1.0-alpha7"
  },
  "require-dev": {
    "drush/drush": "~8.1",
    "drupal/coder": "8.2.12",
    "symfony/css-selector": "~2.8"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "conflict": {
    "drupal/drupal": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-5.x": "8.5.x-dev"
    },
    "installer-paths": {
      "public_html/core": [
        "type:drupal-core"
      ],
      "public_html/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "public_html/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "public_html/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "public_html/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": false,
    "patches": {
      "drupal/ds": {
        "Issue #2883928: Use absolute or external URL for link wrappers":
        "https://www.drupal.org/files/issues/use_absolute_or-2883928-7.patch",
        "Issue #2966959: Value Conflict with Layout Builder":
        "https://www.drupal.org/files/issues/2018-09-24/2966959-22.patch"
      }
    }
  },
  "config": {
    "bin-dir": "bin/"
  },
  "scripts": {
    "post-install-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ],
    "post-update-cmd": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ]
  },
  "authors": [
    {
      "name": "Sprintive",
      "homepage": "http://sprintive.com",
      "role": "Supporting Organization"
    },
    {
      "name": "Mohammad Abdul-Qader",
      "homepage": "https://www.drupal.org/u/mabdulqader",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/seeds",
    "source": "http://cgit.drupalcode.org/seeds",
    "email": "info@sprintive.com"
  }
}
